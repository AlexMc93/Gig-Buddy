{
  "expo": {
    "name": "nc-project",
    "slug": "nc-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./my-app/assets/mini-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#33e4ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./my-app/assets/mini-logo-square.png",
        "backgroundColor": "#33e4ff"
      }
    },
    "web": {
      "favicon": "./my-app/assets/mini-logo.png"
    }
  }
}
